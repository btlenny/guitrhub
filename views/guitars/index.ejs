<%- include('../partials/header') %>

<h2>Guitar List</h2>
<table>
  <tbody>
    <% guitars.forEach(function(g) { %>
      <tr>
        <td class="guitar-container">
          <a href="/guitars/<%= g._id %>">
            <%= g.brand %>  <%= g.model %>
          </a>
          <a href="/guitars/<%= g._id %>/reviews"></a>
        </td>
        <td><a href="/guitars/<%= g._id %>"></a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>